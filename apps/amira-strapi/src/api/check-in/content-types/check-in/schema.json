{
  "kind": "collectionType",
  "collectionName": "check_ins",
  "info": {
    "singularName": "check-in",
    "pluralName": "check-ins",
    "displayName": "Check In"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service"
    },
    "time_in": {
      "type": "datetime",
      "required": true
    },
    "time_out": {
      "type": "datetime"
    },
    "check_in_type": {
      "type": "enumeration",
      "enum": [
        "walk-in",
        "appointment"
      ]
    }
  }
}
